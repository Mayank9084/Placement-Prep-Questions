#include<stdio.h>
#include<conio.h>
void main()
{
 clrscr();
 int i,j;
 char ch= 'A';
 for(i=1;i<=5;i++)
 {
  for(j=1;j<=5;j++)
  {
   if(i==1 || i==5 || j==1 || j==5)
   {
   printf("*");
   }
   else{
	printf("%c",ch++);
	}
	}
	printf("\n");
	}
	getch();
	}


output :-

*****
*ABC*
*DEF*
*GHI*
*****
