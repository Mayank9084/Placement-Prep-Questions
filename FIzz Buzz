#include<conio.h>
#include<stdio.h>
int main()
{
 int a[100],i;
 clrscr();
 for(i=1;i<=100;i++)
 {
 if(i%3==0 && i%5==0)
 {
  printf(" FizzBuzz ");
  }
  else if(i%5==0)
  {
  printf(" Buzz ");
  }
  else if(i%3==0)
  {
  printf(" fizz ");
  }
  else
  printf(" %d ",i);
  }
  getch();
  return 0;
  }
